{
  "assistant": {
    "default_model": {
      "provider": "google",
      "model": "gemini-1.5-flash"
    },
    "version": "2"
  },
  "languages": {
    "Shell Script": {
      "show_inline_completions": true
    }
  },
  "show_inline_completions": true,
  "collaboration_panel": {
    "dock": "right"
  },
  "outline_panel": {
    "dock": "right"
  },
  "vim_mode": true,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Andromeda"
  },
  "terminal": {
    "font_family": "JetBrainsMono Nerd Font",
    "font_size": 14,
    "cursor_shape": "bar"
  },
  "ui_font_family": "Dank Mono",
  "cursor_blink": true,
  "format_on_save": "prettier",
  "cursor_shape": "bar",
  "project_panel": {
    "auto_fold_dirs": true,
    "dock": "right",
    "auto_reveal_entries": true,
    "scrollbar": {
      "show": "never"
    }
  }
}
